{
    "routes": [
        {
            "route": "/404.html",
            "allowedRoles": ["anonymous"]
        },
        {
            "route": "/*",
            "serve": "/index.html",
            "statusCode": 200,
            "allowedRoles": ["manager"]
        }
    ],
    "platformErrorOverrides": [
        {
        "errorType": "Unauthenticated",
        "serve": "/404.html"
        }
    ]
}